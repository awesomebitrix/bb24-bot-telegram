{
  "name": "bb24-bot-telegram",
  "version": "1.0.0-rc",
  "description": "A Telegram bot application for Bale Bale 24",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "main": "dist/main.js",
  "scripts": {
    "app": "node dist/main.js",
    "build": "npm run clean && mkdir dist && npm run compile",
    "clean": "rm -r dist/",
    "compile": "babel src -d dist",
    "start": "npm run build && npm run app",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bb24/bb24-bot-telegram.git"
  },
  "keywords": [
    "telegram",
    "bot"
  ],
  "author": "bb24",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bb24/bb24-bot-telegram/issues"
  },
  "homepage": "https://github.com/bb24/bb24-bot-telegram#readme",
  "dependencies": {
    "b24": "^1.1.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "express": "^4.15.4",
    "lowdb": "^0.17.1",
    "telebot": "^1.2.3"
  }
}
